p,h5{
  color:#fff;
}
/*  清除aui-list-item默认边线*/
.aui-list:before,
.aui-list:after,
.aui-list-item:after,
.aui-list.aui-list-in .aui-list-item-inner:after {
  display: none;
  background-image: url(); /* 开始使用linear-gredient开进行边线描述了 */
}
.aui-list   {
  background-image: url();

}
.aui-list .aui-list-item {
  background-image: url();
}
.aui-list .aui-list-item.aui-list-item-arrow:before {
  content: '';
  width: 0.4rem;
  height: 0.4rem;
  position: absolute;
  top: 50%;
  right: 0.75rem;
  margin-top: -0.2rem;
  background: transparent;
  border: 2px solid #e8e8e8;
  border-top: none;
  border-right: none;
  z-index: 2;
  -webkit-border-radius: 0;
  border-radius: 0;
  -webkit-transform: rotate(-135deg);
  transform: rotate(-135deg);
}


body,
html {
  height: 100%;
  box-sizing: border-box;
  background-color: #fff;
  line-height: 20px;
  color:#fff;
}
i {
  font-style: normal;
}
label {
  font-size: 18px;
}

/* 底部导航栏 */
.sl_footer  {
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  height: 54px;
  background-color: #01042D;
  line-height: 20px;
  table-layout: fixed
}
.sl_footer li  {
  width: 25%;
  text-align: center;
  color:#fff;
}

.sl_footer li > span {
  font-size: 20px;
}
.sl_footer li.active >span,
.sl_footer li.active >p  {
  color:deeppink;
}

main {
  background-color: #010646;
  padding: 10px;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  box-sizing: border-box;
}
.sl_header {
  height: 54px;
  position: relative;
  line-height: 54px;
  background-color: #01042D;
  box-sizing: content-box;
}
/* 分割线 */
.sl_hr {
  height: 5px;
  background-color: #26326C;
  margin-bottom: 20px;
}
/* 镂空效果线 */
.sl_fenge {
  height: 10px;
  border-left: 5px solid #fff;
  width: 100%;
  background-color: #010646;
}
/* 主体渐变色 */
.sl_skin {
  background-image: linear-gradient(90deg, #fccb90 0%, #d57eeb 100%);
  height: 50px;
  border-radius: 5px;
  text-align: center;
  line-height: 50px;
  font-size: 20px;
  color: #fff;
}
.left {
  float: left;
}
.right {
  float: right;
}

/*  改写toast样式 验证码警示 */
i.myToastCon {
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,0);
  display: block;
  width: 300px;
  height: 150px;
  background-color: #eee;
  line-height: 80px;
  border-radius: 5px;
  box-shadow: 0 0 15px rgba(0,0,0,.5);
}
.myToast {
  color:#000528;
  text-align: center;
  width: 100%;
  font-size: 20px;
}
.myToastCon .fa {
  display: inline-block;
  font-size: 60px;
}
.myToastCon .fa.fa-exclamation-triangle {
  color:#FF4B4B
}
/* dialog 覆写 */
.aui-dialog {
  line-height: 50px;
  padding-bottom: 20px;
}

.aui-dialog-header {
  color:#020202;
  font-weight: 700;
}
.ui-dialog-body {
  border: 0;
}
.aui-dialog-btn {
  font-weight: 700;
  width: 25%;
  color:#020202;
   border-radius: 5px;
   background-color: #93D4FD;
}
.aui-dialog-btn:nth-child(1) {
  margin-right: 20px;
}
.aui-dialog-btn:nth-child(2) {
  margin-left: 20px;
}
.aui-dialog-footer {
  border: 0;
  background-image: url();
}


.sl_flex_sb,
.sl_flex_sa,
.sl_flex_c {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.sl_flex_c {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  flex-direction:row;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.sl_flex_1 {
  -webkit-box-flex: 1;
  box-flex: 1;
  -webkit-flex-shrink: 1;
  flex-shrink: 1;
}


.sl_flex_sb {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  flex-direction:row;
  -webkit-flex-direction:row;
  justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.sl_flex_sa {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-around;
  flex-direction:row;
  -webkit-flex-direction:row;
  justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
/* 分栏标题 */
.sl_title {
  border-left: 3px solid #fff;
  font-size: 20px;
  padding: 5px 0 0px 10px;
  margin-bottom: 10px;
}
.sl_news_template {
  padding: 5px;
}
.sl_news_template .sl_news_item {
  margin-bottom: 10px;
  background-color: #111453;
  height: 100px;
  line-height: 20px;
}

/* 快报6页面 */
.fast_news_con {
  width: 90%;
  margin: 0 auto;
  padding: 20px 10px 50px;
  border-left: 3px solid #26326C;
}
.fast_news_item {
  position: relative;
  margin-bottom: 20px;
  color:#fff;

}
.fast_news_item p {
  color:#fff;
}
.fast_news_item h5 {
  color:#fff;
  font-size: 18px;
  margin-bottom: 5px;
}
.fast_news_item::before,
.fast_news_item::after {
  position: absolute;
  content: '';
  display: block;
  border-radius: 50%;
}
.fast_news_item::before{
  top: 0px;
  left: -16px;
  width: 10px;
  height: 10px;
  background-color: #eee;

}
.fast_news_item::after{
  top: 2px;
  left: -14px;
  width: 6px;
  height: 6px;
  background-color: red;
}
